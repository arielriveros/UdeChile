    <servlet-mapping>

        <servlet-name>welcomeServlet</servlet-name>

        <url-pattern>/welcome</url-pattern>

    </servlet-mapping>



    <servlet>

        <servlet-name>fotoCommentsServlet</servlet-name>

        <servlet-class>dcc.cc5002.controller.fotoCommentsServlet</servlet-class>

    </servlet>


    <servlet-mapping>

        <servlet-name>fotoCommentsServlet</servlet-name>

        <url-pattern>/fotos</url-pattern>

    </servlet-mapping>

    <servlet>

        <servlet-name>exitoServlet</servlet-name>

        <servlet-class>dcc.cc5002.controller.exitoServlet</servlet-class>

    </servlet>


    <servlet-mapping>

        <servlet-name>exitoServlet</servlet-name>

        <url-pattern>/exito</url-pattern>

    </servlet-mapping>


</web-app>